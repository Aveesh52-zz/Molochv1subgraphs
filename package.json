{
  "name": "Moloch-subgraphs",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create Aveesh52/Molochv1subgraphs --node https://mumbai-graph.matic.today/",
    "create-local": "graph create Aveesh52/Molochv1subgraphs --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy Aveesh52/Molochv1subgraphs --ipfs https://ipfs.infura.io:5001/ --node https://mumbai-graph.matic.today",
    "deploy-local": "graph deploy Aveesh52/Molochv1subgraphs --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.19.0",
    "@graphprotocol/graph-ts": "^0.19.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}

